let data = {
	pois: {
		<% for (let poi of pois) { %>
			<%= poi.$loki %>: {
				position: <%- JSON.stringify(poi.position) %>,
				title: <%- JSON.stringify(poi.title) %>,
				description: <%- JSON.stringify(poi.description) %>,
				map:  '<%- poi.map %>',
				region: '<%- poi.region %>',
				label: '<%- poi.label %>',
				filelist: [<%- poi.filelist.map(file => `'${ file }'`).join(', ') %>],
				filedata: <%- poi.filedata ? JSON.stringify(poi.filedata) : 'null' %>
			},
		<% } %>
	},

	articles: {
		<% for (let region of Object.keys(regions)) { %>
			<%= region %>: {
				<% for (let map of Object.keys(maps)) { %>
					<%= map %>: {
						<% let article = articles.find(article => article.map == map && article.region == region) %>
						title: <%- article ? JSON.stringify(article.title) : "{ sk: '', en: '' }" %>,
						description: <%- article ? JSON.stringify(article.description) : "{ sk: '', en: '' }" %>,
						filedata: <%- article && article.filedata ? JSON.stringify(article.filedata) : 'null' %>
					},
				<% } %>
			},
		<% } %>
	},

	maps: <%- JSON.stringify(maps) %>,
	regions: <%- JSON.stringify(regions) %>
};

