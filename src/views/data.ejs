let data = {
	<% for (let poi of pois) { %>
		<%= poi.$loki %>: {
			title: '<%- poi.title[lang] %>',
			description: '<%- poi.description[lang] %>',
			group:  '<%- poi.group %>',
			region: '<%- poi.region %>',
			files: [<%- poi.files.map(file => `'${ file }'`).join(', ') %>]
		},
	<% } %>

	articles: {
		<% for (let group in groups) { %>
			<%= group %>: {
				<% for (let region of ['global'].concat(Object.keys(regions))) { %>
					<%= region %>: {
						<% let article = articles.find(article => article.group == group && article.region == region) %>
						title: '<%- article ? article.title[lang] : '' %>',
						description: '<%- article ? article.description[lang] : '' %>'
					},
				<% } %>
			},
		<% } %>
	},

	maps: <%- JSON.stringify(maps) %>,
	groups: <%- JSON.stringify(groups) %>,
	regions: <%- JSON.stringify(regions) %>,

	locale: <%- JSON.stringify(locale) %>,
	lang: '<%= lang %>'
};

